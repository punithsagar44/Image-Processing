   "source": [
    "import cv2 as cv\n",
    "path = 'histogram.png'\n",
    "img = cv.imread(path)\n",
    "img = cv.resize(img, (800, 800))\n",
    "cv.normalize(img, None, 0, 255, cv.NORM_L1)\n",
    "cv.imshow('dst_rt', img)\n",
    "cv.imwrite('norm.png', img)\n",
    "cv.waitKey(0)\n",
    "cv.destroyAllWindows()"
   ]
